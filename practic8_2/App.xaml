<Application x:Class="practic8_2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:practic8_2"
             xmlns:converters="clr-namespace:practic8_2.Converters"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <converters:ImagePathConverter x:Key="imageConverter" />
        <converters:DiscountConverter x:Key="DiscountToPriceConverter" />
        <converters:CountToBrushConverter x:Key="countConverter" />
    </Application.Resources>
</Application>
